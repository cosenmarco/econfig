{
    "mocha.subdirectory": "server/",
    "mocha.requires": [
        "ts-node/register"
    ],
    "mocha.nycPath": "node_modules/.bin/nyc",
    "tslint.packageManager": "yarn",
    "cSpell.words": [
        "econfig",
        "eigen",
        "eigenconfig",
        "error",
        "jsyaml"
    ],
    "javascript.preferences.quoteStyle": "single",
    "typescript.preferences.quoteStyle": "single",
    "typescript.tsdk": "./server/node_modules/typescript/lib",
    "eslint.nodePath": "./server/node_modules/",
    "eslint.packageManager": "yarn",
    "eslint.workingDirectories": ["server"]
}
